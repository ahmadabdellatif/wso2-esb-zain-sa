<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="authenticateEP" xmlns="http://ws.apache.org/ns/synapse">
    <http method="post" uri-template="https://localhost:9443/api/identity/auth/v1.1/authenticate">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
